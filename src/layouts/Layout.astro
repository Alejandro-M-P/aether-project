---
interface Props { title: string; }
const { title } = Astro.props;
---
<!doctype html>
<html lang="es" class="bg-black text-white overflow-hidden">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<title>{title}</title>
	</head>
	<body class="cursor-crosshair">
		<slot />
	</body>
</html>